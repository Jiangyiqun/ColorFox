{
  "manifest_version": 2,
  "name": "Dynamic Icon Theme",
  "version": "0.2",
  "description": "A dynamic theme which shows the icon of current tab",
  "homepage_url": "https://colorfox.jackjyq.com/",
  "permissions": [
    "theme",
    "tabs"
  ],
  "icons": {
    "48": "icon@48.png",
    "96": "icon@96.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "applications": {
    "gecko": {
      "strict_min_version": "55.0a2"
    }
  }
}